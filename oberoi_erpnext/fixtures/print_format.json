  "html": "{% macro above_items() %}\n<div class=\"page-break\" style=\"margin: 15px\">\n        <div class=\"Header\">\n                <div style=\"width: 100%;display: inline-block;border: 1px solid black;border-bottom: 0px;\">\n                    <div style=\"width: 75%;text-align:center;display: inline-block;padding-left: 24%;\">\n                        <!--<div class=\"col-md-9\">{{ frappe.db.get_value(\"sales order\", \"sales_order\"),doc.sales_order }}</div>  -->\n                        <p style=\"font-size: 15px;\">TAX INVOICE</p>\n                        <p style=\"font-size: 10px;\">RULE 46 OF CGST RULES 2017</p>\n                    </div>\n                    <div style=\"width: 20%;float:right;margin-right: 17px;border-left: 1px solid;padding-left: 6px;height: 55px;padding-top: 0px;font-weight: bold;\">\n                        <table class=\"mytable\" ;>\n                            <tr>\n                                <td>\n                                    <p style=\"font-size: 8px;\">[ ] Original for Receipient</br>\n                                        [ ] Duplicate for Transporter</br>\n                                        [ ] Triplicate for Supplier</br>\n                                        [ ] Extra Copy</br>\n                                </td>\n                            </tr>\n                        </table>\n                    </div>\n                </div>\n                <div style=\"width: 100%;display: inline-block;border: 1px solid black;border-bottom: 0px;text-align:center;font-weight: 600;\">\n             {{ letter_head }}\n                </div>\n                <table border=1 width=\"100%\">\n                    <tbody>\n                        <tr>\n                            <td style=\"padding: 0px 6px 0px 6px !important;line-height: 14px;font-weight: 700;\" colspan=1 width=\"160\">\n                                <div>\n                                    <font size=\"1\">Invoice No</font>\n                                </div>\n                                <div>\n                                    <font size=\"1\">Invoice Date</font>\n                                </div>\n                                <div>\n                                    <font size=\"1\">Customer PO</font>\n                                </div>\n                                <div>\n                                    <font size=\"1\">Customer PO Date</font>\n                                </div>\n                            </td>\n                            <td style=\"padding: 0px 6px 0px 6px !important;line-height: 14px;font-weight: 700;\" colspan=1 width=\"200\">\n                                <div>\n                                    <font size=\"1\">\n                                        <b>: {{ doc.name }}</b>\n                                    </font>\n                                </div>\n                                <div>\n                                    <font size=\"1\">\n                                        <b>: {{ doc.get_formatted(\"posting_date\") }}</b>\n                                    </font>\n                                </div>\n                                <div>\n                                    <font size=\"1\">\n                                        <b>: {{ doc.po_no }}</b>\n                                    </font>\n                                </div>\n                                <div>\n                                    <font size=\"1\">\n                                        <b>: {{ doc.get_formatted(\"po_date\") }}</b>\n                                    </font>\n                                </div>\n                            </td>\n                            <td style=\"padding: 0px 6px 0px 6px !important;line-height: 14px;font-weight: 700;\" colspan=1 width=\"160\">\n                                <div>\n                                    <font size=\"1\">Transportation Mode</font>\n                                </div>\n                                <div>\n                                    <font size=\"1\">Vehicle Number</font>\n                                </div>\n                                <div>\n                                    <font size=\"1\">Date of Supply</font>\n                                </div>\n                                <div>\n                                    <font size=\"1\">Place of Supply</font>\n                                </div>\n                            </td>\n                            <td style=\"padding: 0px 6px 0px 6px !important;line-height: 14px;font-weight: 700;\" colspan=1 width=\"200\">\n                                <div>\n                                    <font size=\"1\">\n                                        <b>: </b>\n                                    </font>\n                                </div>\n                                <div>\n                                    <font size=\"1\">\n                                        <b>: </b>\n                                    </font>\n                                </div>\n                                <div>\n                                    <font size=\"1\">\n                                        <b>: {{ doc.get_formatted(\"posting_date\") }}</b>\n                                    </font>\n                                </div>\n                                <div>\n                                    <font size=\"1\">\n                                        <b>: {{ doc.place_of_supply }}</b>\n                                    </font>\n                                </div>\n                            </td>\n                        </tr>\n                    </tbody>\n                </table>\n            </div>\n            <div class=\"seller-info new-seller-info\">\n                <table border=1 style=\"width:100%;\" style=\"border-top: 0px;\">\n                    <tbody>\n                        <tr>\n                            <td style=\" padding: 0px 6px 0px 6px !important;\" width=\"200\" colspan=2>\n                                <div style=\"font-weight:bold;font-size:12px;text-decoration:underline;\">Details of Receiver (Billed To)</div>\n                                <div>\n                                    <b>\n                                        <font size=\"2\">{{ doc.customer_name }}</font>\n                                    </b>\n                                </div>\n                                {% set customer_address = doc.address_display.replace(\"\\n\", \" \").replace(\"<br>\", \" \").replace(\"GSTIN\", \"</br>GSTIN\").replace(\", State Code:\", \".</br>State Code:\") %}\n                                <div>\n                                    <font size=\"1\"> {{ customer_address }} </font>\n                                </div>\n                            </td>\n                            <td style=\" padding: 0px 6px 0px 6px !important;\" width=\"200\" colspan=2>\n                                <div style=\"font-weight:bold;font-size:12px;text-decoration:underline;\">Details of Cosignee(Shipped To)</div>\n                                <div>\n                                    <b>\n                                        <font size=\"2\"> {{ doc.shipping_address_name }}</font>\n                                    </b>\n                                </div>\n                                {% if doc.shipping_address_name -%} {% set shipping_address = doc.shipping_address.replace(\"\\n\", \" \").replace(\"<br>\", \" \").replace(\"GSTIN\", \"</br>GSTIN\") .replace(\", State Code:\", \".</br>State Code:\")%}\n                                <div>\n                                    <font size=\"1\"> {{ shipping_address }} </font>\n                                </div>\n                                {% endif %}\n                            </td>\n                        </tr>\n                    </tbody>\n                </table>\n            </div>\n\n    {% endmacro %} {% macro below_items() %}\n    <table width=\"100%\" border=\"1\" style=\"font-size:9px;border-top:0px;\">\n            <tbody>\n                <tr>\n                    <td width=\"64%\" text-align=\"left\">\n                        <p style=\"text-align: left;\">Total Amount in Words:</br>\n                        </p>\n                        <p style=\"text-align: left;\">{{doc.in_words}}</br>.</p>\n                        <p style=\"text-align: left;\">Bank Details:</br>\n                            A/C No.: 10004983275 </br>\n\t\t\tIFSC Code: IDFB0020151</br>\n                        </p>\n                    </td>\n                    <td style=\"padding: 0px !important;\">\n                        <table width=\"100%\" border=\"0\">\n                            <tbody>\n                                <tr style=\"text-align: right; border-bottom:1px solid\">\n                                    <td>\n                                        <b>Total Amount Before Tax</b>\n                                    </td>\n                                    <td style=\"width:35%;text-align: right; border-left:1px solid;\">{{\"{:,.2f}\".format(grand_total[\"amount\"]) }}</td>\n                                </tr>\n                                <tr style=\"text-align: right; border-bottom:1px solid\">\n                                    <td>\n                                        <b>Add CGST</b>\n                                    </td>\n                                    <td style=\"width:35%;text-align: right; border-left:1px solid;\">{{\"{:,.2f}\".format(grand_total[\"cgst_amt\"]) }}</td>\n                                </tr>\n                                <tr style=\"text-align: right; border-bottom:1px solid\">\n                                    <td>\n                                        <b>Add SGST</b>\n                                    </td>\n                                    <td style=\"width:35%;text-align: right; border-left:1px solid;\">{{\"{:,.2f}\".format(grand_total[\"sgst_amt\"]) }}</td>\n                                </tr>\n                                <tr style=\"text-align: right; border-bottom:1px solid\">\n                                    <td>\n                                        <b>Add IGST</b>\n                                    </td>\n                                    <td style=\"width:35%;text-align: right; border-left:1px solid;\">{{\"{:,.2f}\".format(grand_total[\"igst_amt\"]) }}</td>\n                                </tr>\n                                <tr style=\"text-align: right;\">\n                                    <td>\n                                        <b>Total Amount After Tax</b>\n                                    </td>\n                                    <td style=\"width:35%;text-align: right;border-left:1px solid;\">{{\"{:,.2f}\".format(doc.rounded_total) }}</td>\n                                </tr>\n                            </tbody>\n                        </table>\n                    </td>\n                </tr>\n            </tbody>\n        </table>\n        <div style=\"font-size: 10px\">\n\t\t\n            <div class=\"footer\">\n                <table width=\"100%\" border=\"1\" style=\"font-size:8px;\">\n                    <tbody>\n                        <tr>\n                            <td width=\"70%\" text-align=\"left\" style=\"font-size:8px;\">\n\t\t\t\t  <p style=\"text-align: left;\">Certified that the particulars given above are true &amp; correct and the amount indicated represents\n                                    the price actually charged and that there is no flow of consideration directly or indirectly\n                                    from the buyers.\n                                </p>\n                                <p style=\"text-align: left; font-size:9px;\">TERMS &amp; CONDITIONS :</p>\n                                <p style=\"text-align: left;\"